<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <script src="https://cdn.jsdelivr.net/npm/vue@2.7.0/dist/vue.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

</head>
<body>
    <div>Begin ---></div>

    <div id="textDiv">
        {{message}}
        <button @click="high">Кнопка-{{counter}}</button>
    
    <table border="1">
        <thead>
            <th>id</th>
            <th>Name</th>
        </thead>
        <tbody>
            <tr v-for="user in users">
                <td>{{ user.id }}</td>
                <td>{{ user.name }}</td>
            </tr>
        </tbody>
    </table>
    </div>

    <div>The END...</div>

    <script type="text/javascript">
      let textDiv = new Vue({
        el: "#textDiv",
        data: {
            message: "Hi Sir !",
            users:[
               { id: 1, name: "Den"},
               { id: 2, name: "Bob"},
               { id: 33, name: "Max"},
            ],
            counter: 1,
        },
        created: function() {
            this.counter=4;
            console.log(`created ${this.counter}`);
        },
        mounted: function() {
            this.counter=5;
            console.log(`mounted ${this.counter}`);
            this.high();
        },

        methods: {
            high(){
                this.counter++;
            }
        }
      });
    </script>
    <style>
        div {
            margin: 20px;
        }
    </style>
</body>
</html>